{
  "name": "@algorandfoundation/algokit-client-generator",
  "version": "0.0.0",
  "description": "Generates a TypeScript client for interacting with, and deploying ARC-0032 smart contracts on the Algorand Blockchain.",
  "main": "src/index.js",
  "module": "index.mjs",
  "private": false,
  "engines": {
    "node": ">=20.0"
  },
  "type": "commonjs",
  "scripts": {
    "audit": "better-npm-audit audit",
    "dev": "run-s dev:*",
    "dev:helloworld": "tsx scripts/generate-clients.ts ./examples/smart_contracts/artifacts/hello_world/HelloWorld.arc32.json",
    "dev:reti": "tsx scripts/generate-clients.ts ./examples/smart_contracts/artifacts/reti/Reti.arc56.json",
    "dev:arc56_test": "tsx scripts/generate-clients.ts ./examples/smart_contracts/artifacts/arc56_test/Arc56Test.arc56.json",
    "dev:lifecycle": "tsx scripts/generate-clients.ts ./examples/smart_contracts/artifacts/life_cycle/LifeCycle.arc32.json",
    "dev:minimal": "tsx scripts/generate-clients.ts ./examples/smart_contracts/artifacts/minimal/Minimal.arc32.json",
    "dev:state": "tsx scripts/generate-clients.ts ./examples/smart_contracts/artifacts/state/State.arc32.json",
    "dev:voting_round": "tsx scripts/generate-clients.ts ./examples/smart_contracts/artifacts/voting_round/VotingRound.arc32.json",
    "dev:duplicate_structs": "tsx scripts/generate-clients.ts ./examples/smart_contracts/artifacts/duplicate_structs/DuplicateStructs.arc32.json",
    "dev:nested": "tsx scripts/generate-clients.ts ./examples/smart_contracts/artifacts/nested/Nested.arc56.json",
    "dev:nfd": "tsx scripts/generate-clients.ts ./examples/smart_contracts/artifacts/nfd/Nfd.arc56.json",
    "dev:structs": "tsx scripts/generate-clients.ts ./examples/smart_contracts/artifacts/structs/Structs.arc56.json",
    "dev:zero_coupon_bond": "tsx scripts/generate-clients.ts ./examples/smart_contracts/artifacts/zero_coupon_bond/ZeroCouponBond.arc56.json",
    "build": "run-s build:*",
    "build:0-clean": "rimraf dist coverage",
    "build:1-schema-to-types": "cd src/schema && json2ts application.schema.json > application.d.ts",
    "build:2-lint": "eslint \"src/**/*.ts\" \"examples/**/*.spec.ts\" --max-warnings 0",
    "build:3-check-types": "tsc -p tsconfig.json",
    "build:4-check-types-non-strict": "tsc -p tsconfig.non-strict.json",
    "build:5-build": "rollup -c --configPlugin typescript --configImportAttributesKey with",
    "build:6-copy-pkg-json": "npx --yes @makerx/ts-toolkit@4.0.0-beta.22 copy-package-json --input-folder ./ --output-folder ./dist --main index.js --types index.d.ts --custom-sections module type ",
    "build:7-copy-bin": "copyfiles bin/* dist/bin -f",
    "build:8-copy-readme": "copyfiles ./README.md ./dist",
    "format": "prettier --write .",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "test:watch": "vitest watch",
    "test": "vitest run",
    "test:ci": "TEST_ENV=ci vitest run --coverage",
    "update-approvals": "tsx scripts/update-approvals.ts",
    "contract-build": "poetry run python -m examples",
    "postinstall": "npx --yes patch-package",
    "pre-commit": "npm run lint:fix && npm run test && npm run audit && npm run build"
  },
  "bin": {
    "@algorandfoundation/algokit-client-generator": "bin/cli.mjs",
    "algokitgen": "bin/cli.mjs"
  },
  "author": "Algorand Foundation",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.4.1",
    "change-case": "^5.4.4",
    "commander": "^14.0.0",
    "jsonschema": "^1.5.0"
  },
  "peerDependencies": {
    "@algorandfoundation/algokit-utils": "^9.0.0",
    "algosdk": "^3.2.0"
  },
  "devDependencies": {
    "@algorandfoundation/algokit-utils": "^9.0.0",
    "@algorandfoundation/tealscript": "^0.106.3",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.26.0",
    "@makerx/prettier-config": "^2.0.0",
    "@makerx/ts-config": "1.0.1",
    "@noble/ed25519": "2.2.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@tsconfig/node20": "^20.1.5",
    "@types/wait-on": "^5.3.4",
    "better-npm-audit": "^3.11.0",
    "conventional-changelog-conventionalcommits": "7.0.2",
    "copyfiles": "^2.4.1",
    "eslint-config-prettier": "^10.1.5",
    "hi-base32": "^0.5.1",
    "json-schema-to-typescript": "^13.1.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.2.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.41.1",
    "semantic-release": "^23.0.0",
    "tiny-invariant": "^1.3.1",
    "tsd": "^0.32.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.3",
    "wait-on": "^8.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      {
        "name": "main",
        "prerelease": "beta"
      },
      {
        "name": "release"
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "build",
                "section": "Dependencies and Other Build Updates",
                "hidden": false
              }
            ]
          }
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
